{
    "name": "创建联调分支扩展转审场景",
    "description": "创建联调分支扩展转审场景",
    "metaId": "executor",
    "version": "1.0.8",
    "status": "active",
    "nodes": [
        {
            "metaId": "start1",
            "type": "start",
            "name": "开始",
            "triggerMode": "auto"
        },
        {
            "metaId": "state1",
            "type": "state",
            "name": "CMO审核",
            "task": {
                "type": "APPROVING_TASK",
                "owner": "{{cmo_owner}}",
                "title": "CMO审核",
                "taskId": "97973f0ce81945f2a12b067c6d69c332",
                "created_by": "{{created_by}}",
                "handleRule": "all"
            },
            "flowContext": "{{cmo_operator}}",
            "triggerMode": "manual"
        },
        {
            "metaId": "condi1",
            "type": "condition",
            "name": "判断",
            "triggerMode": "auto"
        },
        {
            "metaId": "state2",
            "type": "state",
            "name": "通知",
            "jober": {
                "name": "测试ECHO_JOBER使用自定义属性",
                "type": "ECHO_JOBER",
                "echoPrefix": "hello: "
            },
            "triggerMode": "auto"
        },
        {
            "metaId": "ender1",
            "type": "end",
            "name": "结束",
            "triggerMode": "auto"
        },
        {
            "metaId": "event1",
            "type": "event",
            "name": "开始",
            "from": "start1",
            "to": "state1"
        },
        {
            "metaId": "event2",
            "type": "event",
            "name": "cmo审核",
            "from": "state1",
            "to": "condi1"
        },
        {
            "metaId": "event3",
            "type": "event",
            "name": "审批通过",
            "from": "condi1",
            "to": "state2",
            "conditionRule": "'{{status}}' == 'approved'"
        },
        {
            "metaId": "event4",
            "type": "event",
            "name": "CMO驳回",
            "from": "condi1",
            "to": "state2",
            "conditionRule": "'{{status}}' == 'rejected'"
        },
        {
            "metaId": "event5",
            "type": "event",
            "name": "CMO转审",
            "from": "condi1",
            "to": "state1",
            "conditionRule": "'{{status}}' == 'transferred'"
        },
        {
            "metaId": "event6",
            "type": "event",
            "name": "结束",
            "from": "state2",
            "to": "ender1"
        }
    ]
}