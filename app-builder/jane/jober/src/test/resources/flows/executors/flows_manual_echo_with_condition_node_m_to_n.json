{
    "name": "流程实例m到n包含condition节点人工任务的场景测试",
    "description": "流程实例m到n包含condition节点人工任务的场景测试",
    "metaId": "executor",
    "version": "1.0.5",
    "status": "active",
    "nodes": [
        {
            "type": "start",
            "metaId": "start1",
            "triggerMode": "auto",
            "name": "开始节点"
        },
        {
            "type": "state",
            "metaId": "state1",
            "name": "创建PM审批任务",
            "triggerMode": "manual",
            "taskFilter": {
                "type": "MINIMUM_SIZE_FILTER",
                "threshold": 2
            },
            "task": {
                "title": "PM审批",
                "task_id": "2479308f35ab4cc492892aea265b2025",
                "type": "APPROVING_TASK",
                "owner": "{{owner1}}",
                "handleRule": "all"
            },
            "joberFilter": {
                "type": "MINIMUM_SIZE_FILTER",
                "threshold": 3
            }
        },
        {
            "type": "condition",
            "metaId": "condition1",
            "name": "PM审批任务",
            "triggerMode": "auto"
        },
        {
            "type": "state",
            "metaId": "state2",
            "name": "echo节点1",
            "triggerMode": "auto",
            "jober": {
                "type": "ECHO_JOBER",
                "name": "发送审批通知",
                "skipVariables": "approve"
            }
        },
        {
            "type": "end",
            "metaId": "ender1",
            "name": "结束节点",
            "triggerMode": "auto"
        },
        {
            "type": "event",
            "metaId": "event1",
            "name": "启动流程",
            "from": "start1",
            "to": "state1"
        },
        {
            "type": "event",
            "metaId": "event2",
            "name": "PM审批任务",
            "from": "state1",
            "to": "condition1"
        },
        {
            "type": "event",
            "metaId": "event3",
            "name": "PM审批任务成功",
            "from": "condition1",
            "to": "state2",
            "conditionRule": "businessData.approve.status"
        },
        {
            "type": "event",
            "metaId": "event4",
            "name": "PM审批任务失败",
            "from": "condition1",
            "to": "ender1",
            "conditionRule": "!businessData.approve.status"
        },
        {
            "type": "event",
            "metaId": "event5",
            "name": "结束流程",
            "from": "state2",
            "to": "ender1"
        }
    ]
}