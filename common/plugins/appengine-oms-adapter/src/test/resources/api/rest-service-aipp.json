[
  {
    "Description": "更新表单数据，并恢复实例任务执行",
    "ResourceKey": "PUT /v1/api/([0-9a-f]{32})/app/instances/([0-9a-f]{32})/log/(\\d+)",
    "SkipCheck": false,
    "Roles": [
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
  {
    "Description": "用户选择历史后启动流程",
    "ResourceKey": "POST /v1/api/([0-9a-f]{32})/start/instances/([0-9a-f]{32})",
    "SkipCheck": false,
    "Roles": [
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
  {
    "Description": "启动一个Aipp",
    "ResourceKey": "POST /v1/api/([0-9a-f]{32})/aipp/([0-9a-f]{32})",
    "SkipCheck": false,
    "Roles": [
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
  {
    "Description": "查询aippd的node节点对应的表单",
    "ResourceKey": "GET /v1/api/([0-9a-f]{32})/aipp/([0-9a-f]{32})/form-metadata/([0-9a-f]{32})",
    "SkipCheck": false,
    "Roles": [
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
  {
    "Description": "启动对话实例",
    "ResourceKey": "POST /v1/api/([0-9a-f]{32})/aipp/([0-9a-f]{32})/start",
    "SkipCheck": false,
    "Roles": [
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
  {
    "Description": "删除应用实例",
    "ResourceKey": "DELETE /v1/api/([0-9a-f]{32})/aipp/([0-9a-f]{32})/instances/([0-9a-f]{32})",
    "SkipCheck": false,
    "Roles": [
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
  {
    "Description": "终止实例任务",
    "ResourceKey": "PUT /v1/api/([0-9a-f]{32})/instances/([0-9a-f]{32})/terminate",
    "SkipCheck": false,
    "Roles": [
      "ApplicationVisitor",
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
  {
    "Description": "查询流程运行时数据",
    "ResourceKey": "GET /v1/api/([0-9a-f]{32})/aipp/([0-9a-f]{32})/instances/([0-9a-f]{32})/runtime",
    "SkipCheck": false,
    "Roles": [
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
  {
    "Description": "根据appid查询对应的aippid和版本号",
    "ResourceKey": "GET /v1/api/([0-9a-f]{32})/app/([0-9a-f]{32})/aipp",
    "SkipCheck": false,
    "Roles": [
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
  {
    "Description": "执行用户代码",
    "ResourceKey": "POST /v1/api/code/run",
    "SkipCheck": false,
    "Roles": [
      "ApplicationDeveloper",
      "FullstackDeveloper",
      "SYS_ADMIN"
    ]
  },
