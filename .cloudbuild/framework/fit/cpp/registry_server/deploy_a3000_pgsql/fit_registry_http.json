    "worker_id": "",
    "environment": "",
    "environment_chain": "",
    "global_open_trace": false,
    "local_open_trace": false,
    "broker_config_file": "{template_app_dir}/conf/broker_configuration.json",
    "registry_address": [
        {
            "host": "127.0.0.1",
            "port": 8003,
            "protocol": 2,
            "id": "{templete_registry_worker_id}",
            "formats": [
                1
            ],
            "environment": "{templete_environment}"
        }
    ],
    "log": {
        "dir": "/applog",
        "filename": "fit-registry",
        "level": "info",
        "stdout": true,
        "file": false,
        "flush-now": false
    },
    "server-port-mapping": [
    ],
    "https": {
        "host": "",
        "port": 8003,
        "protocol": 4,
        "path": "/fit",
        "cer":"",
        "private-key": "",
        "ca-crt": "",
        "private-key-pwd-file": "",
        "pwd-crypto-type": "scc",
        "private-key-pwd": "",
        "ssl-verify": true
    },
    "scc-crypto": {
        "config-file": ""
    },
    "register_service_failed_exit_flag": false,
    "registry-server": {
        "role": "master"
    },
    "is-registry-server": true,
    "pg": {
        "connections": {
            "num": 10,
            "max_retry": 4
        },
        "password-crypto-type":"scc",
        "sslmode": "require"
    },
    "genericable-config-sources": "registry_server,config_file"
}