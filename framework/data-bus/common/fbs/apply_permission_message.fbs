include "common.fbs";

namespace DataBus.Common;

table ApplyPermissionMessage {
    permission: PermissionType;
    object_key: string; // 客户端生成的存储对象UUID
    memory_key: int = -1; // System V生成的共享内存ID；未设定时，默认值为-1
    is_operating_user_data: bool = false; // 本次读写许可申请是否附带对用户数据的操作
    user_data: [byte]; // 用户自定义的字节数组数据
}

root_type ApplyPermissionMessage;