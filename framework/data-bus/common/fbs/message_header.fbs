namespace DataBus.Common;

enum MessageType:byte {
    Unknown = 0,
    Error = 1,
    HeartBeat = 2,
    ApplyMemory = 3,
    ApplyPermission = 4,
    ReleasePermission = 5,
    ReleaseMemory = 6
}

// 修改此 table 请务必与 DataBus 开发组人员确认
table MessageHeader {
