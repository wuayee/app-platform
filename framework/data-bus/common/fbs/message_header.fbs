namespace DataBus.Common;

enum MessageType:byte {
    Unknown = 0,
    Error = 1,
    HeartBeat = 0x10,
    RuntimeReport = 0x11,
    ApplyMemory = 0x20,
    ApplyPermission = 0x21,
    ReleasePermission = 0x30,
    ReleaseMemory = 0x31,
}

// 修改此 table 请务必与 DataBus 开发组人员确认
table MessageHeader {
    type: MessageType = 0;
    size: uint = 0;
}

root_type MessageHeader;