{
  "name": "通用流程",
  "description": "一条包含各种节点和任务类型的流程定义",
  "metaId": "apimckapimckapimckapimckapimckap",
  "version": "1.0.0",
  "status": "active",
  "nodes": [
    {
      "type": "start",
      "metaId": "start1",
      "triggerMode": "auto",
      "name": "开始节点"
    },
    {
      "type": "state",
      "metaId": "state1",
      "name": "测试手动执行task节点",
      "triggerMode": "manual",
      "task": {
        "title": "CMO审批",
        "taskId": "2479308f35ab4cc492892aea265b2025",
        "type": "APPROVING_TASK",
        "owner": "{{owner1}}",
        "created_by": "{{created_by}}",
        "handleRule": "all"
      },
      "joberFilter": {
        "type": "minimum_size_filter",
        "threshold": "5"
      },
      "taskFilter": {
        "type": "minimum_size_filter",
        "threshold": "5"
      },
      "flowContext": "{{outputKey}}"
    },
    {
      "type": "condition",
      "metaId": "condi1",
      "triggerMode": "auto",
      "name": "判断节点"
    },
    {
      "type": "state",
      "metaId": "state2",
      "name": "测试自动执行fitable节点",
      "triggerMode": "auto",
      "jober": {
        "name": "通知",
        "type": "general_jober",
        "fitables": [
          "notify实现"
        ]
      },
      "joberFilter": {
        "type": "minimum_size_filter",
        "threshold": "5"
      },
      "taskFilter": {
        "type": "minimum_size_filter",
        "threshold": "5"
      },
      "callback": {
        "name": "通知回调",
        "type": "general_callback",
        "filteredKeys": [
          "dataType"
        ],
        "fitables": [
          "通知回调函数实现"
        ]
      }
    },
    {
      "type": "state",
      "metaId": "state3",
      "name": "http_jober测试",
      "triggerMode": "auto",
      "jober": {
        "name": "Http调用节点",
        "type": "http_jober",
        "fitables": [
          "http实现"
        ],
        "entity": {
          "httpUrl": "https://localhost:8028/test",
          "httpMethod": "POST",
          "name": "python-fitable",
          "params": "{\"path\": \"D:\\\\project\\\\test.pdf\",\"size\": \"100\"}"
        }
      }
    },
    {
      "type": "end",
      "metaId": "ender1"
      "name": "结束节点",
      "triggerMode": "auto"
    },
    {
      "type": "event",
      "metaId": "event1",
      "name": "开始-手动",
      "from": "start1",
      "to": "state1"
    },
    {
      "type": "event",
      "metaId": "event2",
      "name": "手动-判断",
      "from": "state1",
      "to": "condi1"
    },
    {
      "type": "event",
      "metaId": "event3",
      "name": "审批通过",
      "from": "condi1",
      "to": "state2",
      "variables": [
        "status"
      ],
      "conditionRule": "'{{status}}' == 'approved'"
    },
    {
      "type": "event",
      "metaId": "event4",
      "name": "审批驳回",
      "from": "condi1",
      "to": "ender1",
      "variables": [
        "status"
      ],
      "conditionRule": "'{{status}}' == 'rejected'"
    },
    {
      "type": "event",
      "metaId": "event5",
      "name": "Http调用节点",
      "from": "state2",
      "to": "state3"
    },
    {
      "type": "event",
      "metaId": "event6",
      "name": "结束",
      "from": "state3",
      "to": "ender1"
    }
  ]
}