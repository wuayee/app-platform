var type = safe{ext::context.get(0).get("passData").get("meta").get("fileType")};

if(type.panic_code()>0){
    panic();
}

if(type=="pdf"){
    let context1 = entity{
        .timeout = 30000; .id = "com.huawei.eDataMate.operators.pdf_extractor_plugin";
    };
    let f1 = fit::handleTask(context1);
    ext::context >> f1
}

