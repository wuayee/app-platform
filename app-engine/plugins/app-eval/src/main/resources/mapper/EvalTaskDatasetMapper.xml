<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huawei.jade.app.engine.eval.mapper.EvalTaskDatasetMapper" >
    <resultMap id="EvalTaskDataset" type="com.huawei.jade.app.engine.eval.po.EvalTaskDatasetPo" >
        <result column="eval_task_id" property="evalTaskId"/>
        <result column="eval_dataset_id" property="evalDatasetId"/>
    </resultMap>

    <insert id="insert" parameterType="com.huawei.jade.app.engine.eval.po.EvalTaskDatasetPo">
        insert into eval_task_dataset ("eval_task_id", "eval_dataset_id")
        values (#{evalTaskId}, #{evalDatasetId})
    </insert>
    <insert id="insertAll">
        insert into eval_task_dataset ("eval_task_id", "eval_dataset_id")
        values
        <foreach collection="list" item="item" index="index" separator=", ">
            (#{item.evalTaskId}, #{item.evalDatasetId})
        </foreach>
    </insert>
    <select id="getByEvalTaskId" resultMap="EvalTaskDataset">
        select "eval_task_id", "eval_dataset_id"
        from eval_task_dataset
        where eval_task_id = #{evalTaskId}
    </select>

</mapper>