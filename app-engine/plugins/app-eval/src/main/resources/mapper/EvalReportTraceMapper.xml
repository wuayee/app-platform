<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huawei.jade.app.engine.eval.mapper.EvalReportTraceMapper" >
    <resultMap id="EvalReportTrace" type="com.huawei.jade.app.engine.eval.po.EvalReportTracePo" >
        <result column="id" property="id"/>
        <result column="instance_id" property="instanceId"/>
        <result column="node_id" property="nodeId"/>
        <result column="input" property="input"/>
        <result column="output" property="output"/>
        <result column="time" property="time"/>
        <result column="latency" property="latency"/>
    </resultMap>
    <insert id="insert" parameterType="com.huawei.jade.app.engine.eval.po.EvalReportTracePo" useGeneratedKeys="true" keyProperty="id">
        insert into eval_report_trace ("instance_id", "node_id", "input", "output", "time", "latency")
        values (#{instanceId}, #{nodeId}, #{input}, #{output}, #{time}, #{latency})
    </insert>

    <insert id="insertAll" parameterType="com.huawei.jade.app.engine.eval.po.EvalReportTracePo" useGeneratedKeys="true" keyProperty="id">
        insert into eval_report_trace ("instance_id", "node_id", "input", "output", "time", "latency")
        values
        <foreach collection="list" item="data" separator=",">
            (#{data.instanceId}, #{data.nodeId}, #{data.input}, #{data.output}, #{data.time}, #{data.latency})
        </foreach>
    </insert>

    <select id="getByEvalInstanceId" resultMap="EvalReportTrace">
        select "id", "instance_id", "node_id", "input", "output", "time", "latency"
        from eval_report_trace
        where instance_id = #{instanceId}
    </select>


</mapper>