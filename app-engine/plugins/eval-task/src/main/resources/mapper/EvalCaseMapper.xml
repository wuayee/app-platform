<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huawei.jade.app.engine.task.mapper.EvalCaseMapper">
    <insert id="create" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO t_app_engine_eval_task_case (latency, outcome, instance_id)
        VALUES (#{latency}, #{outcome}, #{instanceId})
    </insert>
</mapper>